def repository = "https://github.com/arpitd143/git_jenkins.git"
String refSpecification = "/main"

def getScm(repoUrl, refSpec) {
    return [
    $claass    :'GitSCM',
    branches   :[[name: refSpec]],
    extensions :[
        
    ]
    ]
}



pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                echo "$env.BUILD_ID"
            }
        }
        stage('build number') {
            steps {
                echo "$env.BUILD_ID"
            }
        }
    }
}
